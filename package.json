{
  "name": "salahtimes",
  "version": "1.0.6",
  "description": "A tiny library to get the islamic prayer times in utc for any given day and location.",
  "main": "salahtimes.js",
  "typings": "salahtimes.d.ts",
  "scripts": {
    "lint": "eslint ./src/** --ext .html,.md,./lib",
    "fix": "eslint ./src/** --ext .html,.md,./lib --fix",
    "clean-docs": "rimraf docs",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "build": "webpack && npm run build-docs",
    "test": "jest",
    "build-docs": "npm run clean-docs && npm run generate-docs",
    "generate-docs": "typedoc --listInvalidSymbolLinks --includeDeclarations --mode file --out docs --readme none --excludeExternals --excludeNotExported --excludePrivate --excludeProtected --ignoreCompilerErrors --plugin typedoc-plugin-markdown --theme markdown",
    "coverage": "jest --coverage --coverageReporters=text",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doniseferi/salahtimes.git"
  },
  "files": [
    "salahtimes.js",
    "salahtimes.map.js"
  ],
  "author": "Edonis Seferi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doniseferi/salahtimes/issues"
  },
  "homepage": "https://github.com/doniseferi/salahtimes#readme",
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.2",
    "typedoc": "^0.16.11",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "suntimes": "^6.0.1"
  }
}
